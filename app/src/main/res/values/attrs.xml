<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="typeface" format="enum">
        <enum name="roboto_medium" value="0" />
        <enum name="roboto" value="1" />
        <enum name="roboto_condensed_bold" value="2" />
        <enum name="helveticaNeue_thin" value="3" />
    </attr>

    <declare-styleable name="MaxHeightScrollView">
        <attr name="maxHeight" format="dimension" />
    </declare-styleable>

    <!-- Drawer Layout -->
    <declare-styleable name="SideBar">
        <attr name="maxTranslationX" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LineChartView">
        <attr name="chart_item_width" format="dimension" />
        <attr name="chart_line_width" format="dimension" />
        <attr name="chart_line_color" format="color" />
        <attr name="chart_point_drawable" format="reference" />
    </declare-styleable>

</resources>